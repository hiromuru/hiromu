%h1 Listing skils

%table
  - @skils.each do |skil|
    %tr
      %td= link_to image_tag(skil.image.image_url.to_s, :class => skil.level), skil
      %td= link_to 'Edit', edit_skil_path(skil)
      %td= link_to 'Destroy', skil, :method => :delete, :data => { :confirm => 'Are you sure?' }

%br

= link_to 'New Skil', new_skil_path
